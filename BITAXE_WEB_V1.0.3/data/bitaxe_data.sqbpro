column index="4" value="62"/><column index="5" value="46"/><column index="6" value="62"/><column index="7" value="39"/><column index="8" value="64"/><column index="9" value="74"/><column index="10" value="48"/><column index="11" value="95"/><column index="12" value="94"/><column index="13" value="93"/><column index="14" value="54"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="tuning_status" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="124"/><column index="2" value="64"/><column index="3" value="74"/><column index="4" value="210"/><column index="5" value="132"/><column index="6" value="63"/><column index="7" value="39"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">-- Tempor√§re View mit Berechnung der Differenzen je IP
CREATE TEMP VIEW logs_deltas AS
SELECT
    timestamp,
    ip,
    sharesAccepted,
    sharesRejected,
    sharesAccepted - LAG(sharesAccepted) OVER (PARTITION BY ip ORDER BY timestamp) AS delta_accepted,
    sharesRejected - LAG(sharesRejected) OVER (PARTITION BY ip ORDER BY timestamp) AS delta_rejected
FROM logs;

-- Tabelle 'logs' aktualisieren mit den berechneten Werten
UPDATE logs
SET
    shares_accepted_delta = (
        SELECT delta_accepted
        FROM logs_deltas AS d
        WHERE d.timestamp = logs.timestamp AND d.ip = logs.ip
    ),
    shares_rejected_delta = (
        SELECT delta_rejected
        FROM logs_deltas AS d
        WHERE d.timestamp = logs.timestamp AND d.ip = logs.ip
    );
</sql><current_tab id="0"/></tab_sql></sqlb_project>
